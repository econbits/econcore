load("auth", "session")
load("datetime", "datetime")
load("fin", "money")
load("iso", "currency")

def login(cred):
    return session()

def accounts(session):
    return [wallet_account("walletid", "walletname", "walletprovider")]

def transactions(session, account, since):
    eur = currency("EUR")
    value = money(1, eur)
    dt = datetime("2006-01-02", "2020-01-30")
    tx = transaction(account, account, value, dt, dt, "purpose")
    return [tx]

